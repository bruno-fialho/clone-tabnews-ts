{
  "name": "clone-tabnews-ts",
  "version": "1.0.0",
  "description": "TabNews clone from [curso.dev](http://curso.dev)! ðŸ“°",
  "main": "index.ts",
  "scripts": {
    "dev": "pnpm services:up && next dev",
    "build": "next build",
    "start": "next start",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "migration:create": "node-pg-migrate -m src/infra/migrations create",
    "migration:up": "node-pg-migrate -m src/infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m src/infra/migrations --envPath .env.development down",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand"
  },
  "author": "Bruno Fialho",
  "license": "MIT",
  "dependencies": {
    "dotenv": "16.4.5",
    "dotenv-expand": "^11.0.6",
    "next": "14.2.33",
    "node-pg-migrate": "6.2.2",
    "pg": "8.11.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/pg": "8.15.6",
    "@types/react": "18.3.3",
    "jest": "29.6.2",
    "prettier": "3.3.3",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
