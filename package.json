{
  "name": "clone-tabnews-ts",
  "version": "1.0.0",
  "description": "TabNews clone from [curso.dev](http://curso.dev)! ðŸ“°",
  "main": "index.ts",
  "scripts": {
    "dev": "pnpm services:up && pnpm services:wait:database && pnpm migrations:up && next dev",
    "postdev": "npm run services:stop",
    "build": "next build",
    "start": "pnpm services:up && pnpm services:wait:database && pnpm migrations:up && next start",
    "poststart": "npm run services:stop",
    "test": "pnpm services:up && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "posttest": "pnpm services:stop",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "lint:eslint:check": "next lint --dir .",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "migrations:create": "node-pg-migrate -m src/infra/migrations create",
    "migrations:up": "node-pg-migrate -m src/infra/migrations --envPath .env.development up",
    "migrations:down": "node-pg-migrate -m src/infra/migrations --envPath .env.development down",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "services:wait:database": "tsx src/infra/scripts/wait-for-postgres.ts",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "Bruno Fialho",
  "license": "MIT",
  "dependencies": {
    "async-retry": "1.3.3",
    "dotenv": "16.4.5",
    "dotenv-expand": "12.0.3",
    "next": "14.2.33",
    "node-pg-migrate": "7.6.1",
    "pg": "8.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/types": "19.8.1",
    "@types/async-retry": "1.4.9",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/pg": "8.15.6",
    "@types/react": "18.3.3",
    "commitizen": "4.3.0",
    "concurrently": "8.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "3.3.3",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
